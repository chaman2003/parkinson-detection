# Development Configuration

# Frontend Development Server
FRONTEND_PORT=8080
FRONTEND_HOST=localhost

# Backend API Server  
BACKEND_PORT=5000
BACKEND_HOST=localhost

# API Configuration
API_BASE_URL=http://localhost:5000/api

# Demo Mode Settings
DEMO_MODE_ENABLED=true
DEMO_RESPONSE_DELAY=2000  # milliseconds

# PWA Configuration
PWA_NAME="Parkinson's Detection PWA"
PWA_SHORT_NAME="Parkinson's Detection"
PWA_THEME_COLOR="#2196F3"
PWA_BACKGROUND_COLOR="#667eea"

# Cache Configuration
CACHE_VERSION="v1.0.0"
STATIC_CACHE_NAME="parkinson-detection-v1.0.0"
API_CACHE_NAME="parkinson-api-v1.0.0"

# ML Model Configuration
VOICE_MODEL_VERSION="1.0.0"
TREMOR_MODEL_VERSION="1.0.0"
ENSEMBLE_WEIGHTS_VOICE=0.6
ENSEMBLE_WEIGHTS_TREMOR=0.4

# Audio Processing Settings
SAMPLE_RATE=22050
RECORDING_DURATION_VOICE=10000  # milliseconds
RECORDING_DURATION_TREMOR=15000  # milliseconds

# Feature Extraction Settings
MFCC_COEFFICIENTS=13
SPECTRAL_CONTRAST_BANDS=7
TREMOR_FREQUENCY_RANGE_MIN=3  # Hz
TREMOR_FREQUENCY_RANGE_MAX=12  # Hz

# Performance Settings
MAX_UPLOAD_SIZE=16  # MB
PROCESSING_TIMEOUT=30000  # milliseconds
BACKGROUND_SYNC_ENABLED=true

# Development Flags
DEBUG_MODE=true
VERBOSE_LOGGING=true
MOCK_DATA_ENABLED=true